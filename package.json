{
  "name": "alc-qa",
  "version": "1.0.0",
  "description": "Test framework for automating mobile apps with appium using webdriverio & typescript.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf typescript/ && rimraf allure-results/",
    "clean-build": "npm run clean && npm run build",
    "appium": "appium",
    "appium-chrome": "appium --allow-insecure chromedriver_autodownload",
    "app-test-tag": "wdio ./config/wdio.app.config.js --cucumberOpts.tagExpression \"@run\"",
    "relay-anthem": "wdio ./config/wdio.relay-anthem.config.js --cucumberOpts.tagExpression \"@run\"",
    "app-test": "wdio ./config/wdio.app.config.js",
    "nautilus": "wdio ./config/wdio.app.config.js --cucumberOpts.tagExpression \"@run\"",
    "report": "allure generate ./allure-results",
    "start-app-gotham": "npm run clean-build && npm run tripkit-gotham",
    "start-app-nautilus": "npm run clean-build && npm run nautilus",
    "start-web-anthem": "npm run clean-build && npm run relay-anthem",
    "clean-report": "allure generate --clean ./allure-results",
    "open-report": "allure open"
  },
  "keywords": [
    "appium",
    "webdriverio",
    "typescript",
    "mobile automation",
    "behaviour driven development",
    "bdd",
    "selenium",
    "webdriverJS",
    "gherkin",
    "automation testing"
  ],
  "author": "Carlos Gauto <cgauto@baufest.com>",
  "license": "MIT",
  "dependencies": {
    "@types/chai": "^4.1.4",
    "@types/cucumber": "^7.0.0",
    "@types/faker": "^4.1.12",
    "@types/uuid": "^7.0.3",
    "@types/webdriverio": "^4.10.2",
    "@wdio/allure-reporter": "^6.11.0",
    "@wdio/appium-service": "^6.12.1",
    "@wdio/cli": "^7.16.3",
    "@wdio/cucumber-framework": "^7.9.0",
    "@wdio/local-runner": "^6.12.1",
    "@wdio/spec-reporter": "^6.11.0",
    "@wdio/sync": "^6.11.0",
    "allure": "0.0.0",
    "allure-commandline": "^2.5.0",
    "appium": "^1.19.1",
    "appium-chromedriver": "^4.25.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "chromedriver": "^89.0.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "localtunnel": "^2.0.1",
    "ngrok": "^3.4.0",
    "nodejs": "0.0.0",
    "rimraf": "^2.6.2",
    "ts-node": "^6.1.1",
    "tslint": "^5.20.1",
    "twilio": "^3.55.1",
    "typescript": "^3.9.9",
    "update": "^0.7.4",
    "uuid": "^7.0.3",
    "wdio-chromedriver-service": "^6.0.3"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^8.0.0-rc.1",
    "@types/jest": "^27.0.2",
    "@types/jquery": "^3.5.8",
    "@types/node": "^10.17.60",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "chai": "^4.3.4",
    "eslint": "^7.22.0"
  }
}
